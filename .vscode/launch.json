{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch",
            "type": "node",
            "request": "launch",
            "program": "${workspaceFolder}/src/www.ts",
            "stopOnEntry": false,
            "args": [],
            "cwd": "${workspaceFolder}",
            "runtimeExecutable": null,
            "runtimeArgs": [
                "--nolazy"
            ],
            "env": {
                "NODE_ENV": "development"
            },
            "console":"integratedTerminal",
            "sourceMaps": true, 
          "outFiles": [
              "${workspaceFolder}/out/*"
          ],
          "protocol": "inspector"  
        },
        {
            "name": "Attach", 
            "type": "node", 
            "request": "attach", 
            "port": 5858
        }
    ],
    "compounds": []
}
